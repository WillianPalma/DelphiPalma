  object qryFechaPims: TOraQuery
    Session = cnxConexaoFechaPims
    SQL.Strings = (
      'SELECT A.DESCRICAO AS PROCESSO,'
      '       A.DATA_PROCESSO AS DATA,'
      '       A.SECAO,'
      '       A.ENTRADA,'
      '       CASE'
      
        '         WHEN (B.DATA_ATUAL_FORMATADA - A.DATA_PROCESSO) > 3 THE' +
        'N'
      '          '#39'ATEN'#199#195'O! PROCESSO EM ATRASO'#39
      '         ELSE'
      '          '#39'PROCESSAMENTO OK!'#39
      '       END STATUS'
      '  FROM (SELECT P.SECAO AS SECAO,'
      '               P.ENTRADA AS ENTRADA,'
      '               TO_DATE(P.VALOR) AS DATA_PROCESSO,'
      '               P.DESCRICAO'
      '          FROM PIMSCS.PARAMETROS P'
      '         WHERE P.SECAO IN ('#39'ATRC_MEC'#39','
      '                           '#39'ATRC_PRD'#39','
      '                           '#39'ATRC_INS'#39','
      '                           '#39'RCMP_PRDOP'#39','
      '                           '#39'ATRC_MAN'#39','
      '                           '#39'COLCAM'#39','
      '                           '#39'RCMP_FLUXO'#39','
      '                           '#39'MNF_LUB'#39','
      '                           '#39'RCMP'#39','
      '                           '#39'ATRC_MDI'#39','
      '                           '#39'PRCL_ANM'#39','
      '                           '#39'AGRO_CLIM'#39','
      '                           '#39'AGRO_BROC'#39','
      '                           '#39'AGRO_FORMIGA'#39','
      '                           '#39'ATMEC_RCMP'#39','
      '                           '#39'CSTG_C'#39','
      '                           '#39'MNF_MAN'#39','
      '                           '#39'MNF_PNEUS'#39','
      '                           '#39'ATRC_PLAN'#39','
      '                           '#39'PRCL_MAT'#39','
      '                           '#39'RCMP_PERDA'#39','
      '                           '#39'MNF_ABAST'#39')'
      '           AND P.ENTRADA IN ('#39'DT_PROCESSO'#39','
      '                             '#39'DT_HISTMDO'#39','
      '                             '#39'DT_HIST_COLCAM'#39','
      '                             '#39'DT_PROC_DISTCOM'#39','
      '                             '#39'DT_PROC_AGRE'#39','
      '                             '#39'DT_PROC_PROD'#39','
      '                             '#39'DT_PROC_AREA'#39','
      '                             '#39'DT_PROC_PRECIP'#39','
      '                             '#39'DT_MOVTO'#39','
      '                             '#39'DT_PROC_Z_PRECIP'#39','
      '                             '#39'ATMEC_RCMP'#39','
      '                             '#39'RCMP_FLUXO'#39','
      '                             '#39'DT_GERA_ATIVMEC'#39','
      '                             '#39'DT_PRODOPER'#39','
      '                             '#39'ATMEC_RCMP'#39','
      '                             '#39'DT_PROCESSO'#39','
      '                             '#39'DT_PROC_APT_FUNC'#39','
      '                             '#39'DT_PROC_BX_PL_SAFRA'#39','
      '                             '#39'DT_PROC_GAR_EQUIPTO'#39','
      '                             '#39'DT_PROC_OS_SEM_APT'#39','
      '                             '#39'DT_PROC_SERVPREVEXEC'#39','
      '                             '#39'DT_PROC_TEMPO_PER'#39','
      '                             '#39'DT_HISTBOLETIM'#39')) A,'
      
        '       (SELECT TO_DATE(TO_CHAR(SYSDATE, '#39'DD/MM/YYYY'#39'), '#39'DD/MM/YY' +
        'YY'#39') AS DATA_ATUAL_FORMATADA'
      '          FROM DUAL) B'
      ' WHERE A.DESCRICAO NOT IN'
      
        '       ('#39'Data do '#250'ltimo processamento de distribui'#231#227'o de combust' +
        #237'vel/lubrificante pelo Manfro para equipamentos no Plantio'#39','
      '        '#39'Data para gera'#231#227'o de agreg. para apto de Lubrifica'#231#227'o'#39')'
      'UNION ALL'
      'SELECT A.DESCRICAO AS PROCESSO,'
      '       A.DATA_PROCESSO AS DATA,'
      '       A.SECAO,'
      '       A.ENTRADA,'
      '       CASE'
      
        '         WHEN (B.DATA_ATUAL_FORMATADA - A.DATA_PROCESSO) > 3 THE' +
        'N'
      '          '#39'ATEN'#199#195'O! PROCESSO EM ATRASO'#39
      '         ELSE'
      '          '#39'PROCESSAMENTO OK!'#39
      '       END STATUS'
      '  FROM (SELECT P.SECAO AS SECAO,'
      '               P.ENTRADA AS ENTRADA,'
      '               TO_DATE(P.VALOR) AS DATA_PROCESSO,'
      '               P.DESCRICAO'
      '          FROM PIMSCS.PARAMETROS P'
      '         WHERE P.SECAO = '#39'MNF_LUB'#39
      '           AND P.ENTRADA = '#39'DT_PROC_AGRE'#39
      
        '           AND P.VALIDOS = '#39'data (formato local da instala'#231#227'o)'#39')' +
        ' A,'
      
        '       (SELECT TO_DATE(TO_CHAR(SYSDATE, '#39'DD/MM/YYYY'#39'), '#39'DD/MM/YY' +
        'YY'#39') AS DATA_ATUAL_FORMATADA'
      '          FROM DUAL) B'
      ' ORDER BY 2 ASC, 3, 4')
